{
  "name": "google-font-metadata",
  "version": "1.4.0",
  "author": "Lotus <declininglotus@gmail.com>",
  "license": "MIT",
  "description": "A metadata generator for Google Fonts.",
  "keywords": [
    "fontsource",
    "font",
    "font family",
    "google fonts",
    "metadata",
    "fonts api",
    "unicode range"
  ],
  "main": "./lib/index",
  "types": "./lib/index",
  "devDependencies": {
    "@types/async": "^3.2.3",
    "@types/cheerio": "^0.22.22",
    "@types/jsonfile": "^6.0.0",
    "@types/lodash": "^4.14.162",
    "@types/node": "^14.14.0",
    "@types/puppeteer": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "4.4.1",
    "@typescript-eslint/parser": "^4.5.0",
    "conventional-changelog-cli": "^2.1.0",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "async": "^3.2.0",
    "axios": "^0.20.0",
    "cheerio": "^1.0.0-rc.3",
    "jsonfile": "^6.0.1",
    "lodash": "^4.17.20",
    "postcss": "^8.1.1",
    "puppeteer": "^5.3.1",
    "retry-axios": "^2.2.1"
  },
  "scripts": {
    "compile": "tsc",
    "parser:v1": "node ./src/api-parser-v1.js",
    "parser:v2": "node ./src/api-parser-v2.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "variable:gen": "node ./src/variable-gen.js",
    "variable:parse": "node ./src/variable-parser.js",
    "format": "yarn format:prettier && yarn format:eslint",
    "format:src": "prettier --write ./src/*.{js,ts,json,md}",
    "format:eslint": "eslint **/*.{js,ts} --fix",
    "format:prettier": "prettier --write **/*.{js,ts,json,md}"
  },
  "homepage": "https://github.com/DecliningLotus/google-font-metadata",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DecliningLotus/google-font-metadata"
  }
}
